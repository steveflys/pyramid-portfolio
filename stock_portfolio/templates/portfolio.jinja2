
{% extends "base.jinja2" %}

{% block content %}
<h1>Your Portfolio</h1>

<ul>
  {% for entry in entries %}
  <li id="stock_short">
    <h2>{{entry.companyName}}</h2>
    <p>({{entry.symbol}}) {{entry.website}}</p>
    <p id="detail_short"><a href="{{request.route_url('detail', symbol=entry.symbol)}}">view details</a></p>
  </li>
  {% endfor %}
</ul>
{% endblock content %}
