
{% extends "base.jinja2" %}

{% block content %}
<h1>Your Portfolio</h1>

<ul>
  {% for stock in stocks %}
  <li>
    <h2>{{stock.companyName}}</h2>
    <p>({{stock.symbol}}) {{stock.website}}</p>
    <p><a href="{{request.route_url('detail', symbol=stock.symbol)}}">view details</a></p>
  </li>
  {% endfor %}
</ul>
{% endblock %}
